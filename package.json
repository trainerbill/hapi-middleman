{
  "name": "hapi-middleman",
  "version": "0.0.0",
  "description": "hapi-middleman",
  "license": "MIT",
  "repository": "",
  "author": "Andrew Throener",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "build:quick": "npm run clean && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive 'test/**/*-spec.ts'",
    "coverage": "nyc --include='src/**/*.ts' --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register --recursive 'test/**/*.test.ts'",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "start": "node -r dotenv/config lib/index.js",
    "reinstall": "rimraf node_modules package-lock.json && npm install",
    "release": "npm test && npm run build && npm version --",
    "release:quick": "npm test && npm run build && git add -A && git commit -m 'Quick Release Patch' && npm version patch && git push --tags",
    "postversion": "npm publish && git push && git push --tags"
  },
  "dependencies": {
    "catbox-mongodb": "^3.0.1",
    "dotenv": "^4.0.0",
    "glue": "^4.2.0",
    "good": "^7.2.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.4.3",
    "hapi-intacct": "0.0.8",
    "hapi-paypal": "0.0.41",
    "later": "^1.2.0",
    "mongoose": "^4.11.5",
    "therealyou": "^1.1.0",
    "us": "^2.0.0",
    "wozu": "^1.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.0",
    "@types/hapi": "^16.1.6",
    "@types/later": "^1.2.3",
    "@types/mocha": "^2.0.0",
    "@types/mongoose": "^4.7.19",
    "@types/node": "^7.0.0",
    "chai": "^3.0.0",
    "coveralls": "^2.0.0",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.0.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}
